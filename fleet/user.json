{
  "keymap": [
    {
      "key": "alt-q",
      "action": "terminal/toggle"
    },
    {
      "key": "alt-k",
      "action": "open-keymap"
    },
    {
      "key": "shift-cmd-n",
      "action": "new-folder"
    },
    {
      "key": "alt-a",
      "action": "files/rename"
    },
    {
      "key": "alt-cmd-l",
      "action": "reformat-code"
    },
    {
      "key": "shift-cmd-w",
      "action": "close-window"
    },
    {
      "key": "alt-c",
      "action": "git/commit/jump-to-tool"
    },
    {
      "key": "alt-tab",
      "action": "jump-to-files-tree"
    },
    {
      "key": "alt-h",
      "action": "git/history/jump-to-tool"
    },
    {
      "key": "shift-alt-q",
      "action": "terminal/new-tool"
    }
  ]
}
