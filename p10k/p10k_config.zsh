# ===== P10K COLOURS =====

# Base
c1=238
c2=230
c3=221
c4=222
c5=216
c6=157
c7=202

# VCS
export CLEAN_BG=$c6
export MODIFIED_BG=$c4
export UNTRACKED_BG=$c5
export CONFLICTED_BG=$c7
export LOADING_BG=$c1
export META_FG=$c1
export CLEAN_FG=$c1
export MODIFIED_FG=$c1
export UNTRACKED_FG=$c1
export CONFLICTED_FG=$c2
export VCS_FG=$c1

# OS Icon
export OS_BG=$c2
export OS_FG=$c1

# Current Directory
export DIR_BG=$c1
export DIR_FG=$c2

# Execution Time
export EXEC_BG=$c2
export EXEC_FG=$c1

# Context
export CONTEXT_BG=$c1
export CONTEXT_FG=$c2

# Current time
export TIME_BG=$c2
export TIME_FG=$c1

# POWERLEVEL9K_INSTANT_PROMPT mode (verbose|quiet|off)
export INSTANT_PROMPT_MODE=quiet
