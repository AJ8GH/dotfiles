{
  "keymap": [
    {
      "key": "alt-q",
      "trigger": "toggle-terminal"
    },
    {
      "key": "alt-k",
      "trigger": "edit-keymap"
    },
    {
      "key": "shift-cmd-n",
      "trigger": "new-folder"
    },
    {
      "key": "alt-a",
      "trigger": "workspace-tree/rename-file"
    },
    {
      "key": "alt-cmd-l",
      "trigger": "reformat-code"
    },
    {
      "key": "shift-cmd-w",
      "trigger": "close-window"
    },
    {
      "key": "alt-c",
      "trigger": "git/commit"
    },
    {
      "key": "alt-tab",
      "trigger": "jump-to-files-tree"
    },
    {
      "key": "alt-h",
      "trigger": "git/jump-to-history"
    },
    {
      "key": "shift-alt-q",
      "trigger": "new-terminal"
    }
  ]
}
